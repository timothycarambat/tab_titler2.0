{
  "manifest_version": 2,
  "name": "Tab Titler 2.0",
  "version": "1.0",
  "description": "Tab Titler 2.0",
  "icons": {
    "16": "tab16.png",
    "48": "tab48.png",
    "128": "tab128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
   {
     "matches": ["*://*/*"],
     "js": ["main.js"]
   }
 ],
 "commands": {
    "rename_tab" : {
        "suggested_key": {
             "default": "Alt+Shift+S"
        },
        "description": "Save a Tab Name"
    }
  },
  "options_ui": {
      "page": "options.html",
      "open_in_tab": true
  }
}
